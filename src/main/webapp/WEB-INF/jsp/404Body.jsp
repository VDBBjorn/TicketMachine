<%@taglib uri="http://www.springframework.org/tags/form" prefix="form"%>
<%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %>

<section id="error404-wrapper" class="section">
    <div class="container">
        <div class="row">
            <div class="col-md-6 wrapper-1 hidden-xs">
                <div class="error404">
                    <span>404</span>
                </div>
            </div>
            <div class="col-md-6 wrapper-1 hidden-sm hidden-md hidden-lg">
                <div class="error404-small">
                    <span>404</span>
                </div>
            </div>
            <div class="col-md-6 wrapper-1">
                <div class="details404">
                    <h3><spring:message code="404.message"/></h3>
                    <a href="${pageContext.request.contextPath}/index" class="btn btn-flat flat-color"><i class="icon-long-arrow-left"></i><spring:message code="404.backToHome"/></a>
                </div>
            </div>
        </div>
    </div>
</section>





